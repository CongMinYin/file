{
    "NovaServers.boot_and_delete_server": [
        {
            "args": {
                "flavor": {
                    "name": "flavor-windows"
                },
                "image": {
                    "name": "windows-snapshot"
                },
                "force_delete": false
            },
            "runner": {
                "type": "constant",
                "times": 16,
                "concurrency": 4
            },
            "context": {
                "users": {
                    "tenants": 4,
                    "users_per_tenant": 2
                }
            }
        }
    ]
}
